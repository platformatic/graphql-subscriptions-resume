{
  "name": "@platformatic/graphql-subscriptions-resume",
  "version": "0.1.0",
  "description": "An addon to @fastify/http-proxy to resume GraphQL Subscriptions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "node --test --experimental-strip-types test/*.test.ts",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "build": "tsc -p tsconfig.build.json",
    "test:coverage": "c8 -c test/config/c8-local.json node --env-file=test/config/env --no-warnings --test --test-timeout=300000 'test/**/*.test.ts'",
    "test:ci": "c8 -c test/config/c8-ci.json node --env-file=test/config/env --no-warnings --test --test-timeout=300000 'test/**/*.test.ts'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist;",
    "check": "pnpm clean && pnpm lint && pnpm typecheck && pnpm test && pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platformatic/graphql-subscriptions-resume.git"
  },
  "author": "Platformatic Inc. <oss@platformatic.dev> (https://platformatic.dev)",
  "dependencies": {
    "abstract-logging": "^2.0.1",
    "cleaner-spec-reporter": "^0.5.0",
    "graphql": "^16.11.0",
    "pino": "^9.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "c8": "^10.1.3",
    "eslint": "^9.34.0",
    "neostandard": "^0.12.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
